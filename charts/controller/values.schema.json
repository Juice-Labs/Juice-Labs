{
    "$schema": "https://json-schema.org/draft/2020-12/schema#",
    "title": "Values",
    "type": "object",
    "required": [
        "frontend",
        "backend",
        "image",
        "service"
    ],
    "additionalProperties": false,
    "properties": {
        "image": {
            "description": "",
            "type": "string"
        },
        "postgresql": {
            "description": "",
            "type": "object",
            "required": [
                "user",
                "password",
                "dbname",
                "host"
            ],
            "additionalProperties": false,
            "properties": {
                "user": {
                    "description": "",
                    "type": "string"
                },
                "password": {
                    "description": "",
                    "type": "string"
                },
                "dbname": {
                    "description": "",
                    "type": "string"
                },
                "host": {
                    "description": "",
                    "type": "string"
                },
                "port": {
                    "description": "",
                    "type": "number"
                },
                "sslmode": {
                    "description": "",
                    "type": "string",
                    "enum": [
                        "disable",
                        "require",
                        "verify-ca",
                        "verify-full"
                    ]
                },
                "connect_timeout": {
                    "description": "",
                    "type": "number"
                },
                "sslcert": {
                    "description": "",
                    "type": "string"
                },
                "sslkey": {
                    "description": "",
                    "type": "string"
                },
                "sslrootcert": {
                    "description": "",
                    "type": "string"
                }
            }
        },
        "frontend": {
            "description": "",
            "type": "object",
            "required": [
                "name",
                "replicas",
                "port"
            ],
            "additionalProperties": false,
            "properties": {
                "name": {
                    "description": "The metadata name for the frontend deployment",
                    "type": "string"
                },
                "port": {
                    "description": "",
                    "type": "number"
                },
                "replicas": {
                    "description": "",
                    "type": "number"
                },
                "additionalLabels": { "#ref": "#/$defs/additionalSpec" },
                "additionalSpec": { "#ref": "#/$defs/additionalSpec" },
                "additionalTemplateLabels": { "#ref": "#/$defs/additionalTemplateLabels" },
                "additionalTemplateSpec": { "#ref": "#/$defs/additionalTemplateSpec" },
                "additionalControllerSpec": { "#ref": "#/$defs/additionalControllerSpec" },
                "additionalControllerArgs": { "#ref": "#/$defs/additionalControllerArgs" }
            }
        },
        "backend": {
            "description": "",
            "type": "object",
            "required": [
                "name"
            ],
            "additionalProperties": false,
            "properties": {
                "name": {
                    "description": "The metadata name for the backend deployment",
                    "type": "string"
                },
                "additionalLabels": { "#ref": "#/$defs/additionalSpec" },
                "additionalSpec": { "#ref": "#/$defs/additionalSpec" },
                "additionalTemplateLabels": { "#ref": "#/$defs/additionalTemplateLabels" },
                "additionalTemplateSpec": { "#ref": "#/$defs/additionalTemplateSpec" },
                "additionalControllerSpec": { "#ref": "#/$defs/additionalControllerSpec" },
                "additionalControllerArgs": { "#ref": "#/$defs/additionalControllerArgs" }
            }
        },
        "common": {
            "description": "",
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "additionalLabels": { "#ref": "#/$defs/additionalSpec" },
                "additionalSpec": { "#ref": "#/$defs/additionalSpec" },
                "additionalTemplateLabels": { "#ref": "#/$defs/additionalTemplateLabels" },
                "additionalTemplateSpec": { "#ref": "#/$defs/additionalTemplateSpec" },
                "additionalControllerSpec": { "#ref": "#/$defs/additionalControllerSpec" },
                "additionalControllerArgs": { "#ref": "#/$defs/additionalControllerArgs" }
            }
        },
        "service": {
            "description": "",
            "type": "object",
            "additionalProperties": false,
            "required": [
                "name",
                "port"
            ],
            "properties": {
                "name": {
                    "description": "The metadata name for the backend deployment",
                    "type": "string"
                },
                "port": {
                    "description": "",
                    "type": "number"
                },
                "additionalSpec": { "#ref": "#/$defs/additionalServiceSpec" }
            }
        }
    },

    "$defs": {
        "additionalLabels": {
            "description": "",
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "app.juicelabs.co/controller": false
            }
        },
        "additionalSpec": {
            "description": "",
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "replicas": false,
                "selector": false
            }
        },
        "additionalTemplateLabels": {
            "description": "",
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "app.juicelabs.co/controller": false
            }
        },
        "additionalTemplateSpec": {
            "description": "",
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "containers": false
            }
        },
        "additionalControllerSpec": {
            "description": "",
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "name": false,
                "image": false,
                "args": false
            }
        },
        "additionalControllerArgs": {
            "description": "",
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "additionalServiceSpec": {
            "description": "",
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "ports": false,
                "selector": false
            }
        }
    }
}
